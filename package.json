{
  "name": "recycling-game-widget",
  "version": "0.0.12",
  "description": "Embeddable Recycling Game as a Web Component",
  "main": "dist/recycling-game-widget.js",
  "module": "dist/recycling-game-widget.js",
  "keywords": [
    "recycling",
    "game",
    "widget",
    "web-component",
    "react"
  ],
  "files": [
    "dist"
  ],
  "author": "Suman Kunwar",
  "license": "MIT",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "react-scripts": "^5.0.1",
    "react-to-webcomponent": "^2.0.1",
    "styled-components": "^5.0.0",
    "uifx": "^2.0.7"
  },
  "scripts": {
    "start": "craco start",
    "build_old": "craco build",
    "build": "PUBLIC_URL=https://unpkg.com/recycling-game-widget@$npm_package_version/dist craco build && node build-widget.js",
    "prepublishOnly": "npm run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start-sw": "http-server ./build",
    "start-rewired": "react-app-rewired start",
    "build-rewired": "react-app-rewired build",
    "test-rewired": "react-app-rewired test",
    "report-coverage": "npm test -- --coverage --watchAll=false && codecov"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "codecov": "^3.6.1",
    "css-loader": "^7.1.2",
    "http-server": "^0.12.1",
    "mini-css-extract-plugin": "^2.9.4",
    "react-app-rewired": "^2.2.1"
  },
  "homepage": "https://unpkg.com/recycling-game-widget@$npm_package_version/dist/",
  "engines": {
    "npm": "10.1.0",
    "node": "v20.9.0"
  }
}
